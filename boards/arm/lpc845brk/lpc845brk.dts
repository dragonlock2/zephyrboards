/dts-v1/;

#include <nxp/nxp_lpc845.dtsi>

/ {
    model = "LPC845-BRK Development Board";
    compatible = "nxp,lpc";

    chosen {
        zephyr,sram = &sram0;
        zephyr,flash = &flash0;
        zephyr,console = &uart0;
    };
};

&uart0 {
    pinctrl = <P0_25 P0_24>;
    current-speed = <115200>;
    status = "okay";
};
